{
  "name": "myslider",
  "version": "1.0.0",
  "description": "mvp slider",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "test": "jest ./src/tests",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "scss": "node-sass -o ./dist/plugin ./src/blocks/plugin",
    "plugin": "webpack --mode development ./src/blocks/plugin/plugin.ts --output ./dist/plugin/plugin.js"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/tests"
    ],
    "verbose": true,
    "collectCoverage": true,
    "testEnvironment": "jest-environment-jsdom-global",
    "coverageReporters": [
      "text",
      "html"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "author": "Samvel Saakyan",
  "license": "ISC",
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "webpack.config.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^5.8.0",
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.5.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.5.4",
    "jest-environment-jsdom": "^25.5.0",
    "jest-environment-jsdom-global": "^2.0.2",
    "jsdom": "^16.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "2.0.5",
    "pug": "^2.0.4",
    "pug-html-loader": "^1.1.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  }
}
